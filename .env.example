# 服務器配置
PORT=8700
VITE_API_BASE_URL=http://localhost:8700/api

# MongoDB 連接URL
MongoDB_url=mongodb://localhost:27017/your-database-name

# 會話密鑰
SESSION_SECRET=your-session-secret-key

# Cloudflare R2 存儲配置
R2_REGION=auto
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-access-key-id
R2_SECRET_ACCESS_KEY=your-secret-access-key
R2_BUCKET=your-bucket-name
R2_PUBLIC_URL=https://your-public-url.com

# ===== UberEats Sandbox 環境 (測試用) =====
UBEREATS_ENVIRONMENT=sandbox
UBEREATS_SANDBOX_CLIENT_ID=your_sandbox_client_id_from_uber_developer_dashboard
UBEREATS_SANDBOX_CLIENT_SECRET=your_sandbox_client_secret_from_uber_developer_dashboard
UBEREATS_SANDBOX_WEBHOOK_SECRET=your_sandbox_webhook_secret
UBEREATS_SANDBOX_STORE_ID=your_test_store_id_from_uber

# ===== UberEats Production 環境 (正式用，暫時註解) =====
# UBEREATS_ENVIRONMENT=production
# UBEREATS_PRODUCTION_CLIENT_ID=your_production_client_id
# UBEREATS_PRODUCTION_CLIENT_SECRET=your_production_client_secret
# UBEREATS_PRODUCTION_WEBHOOK_SECRET=your_production_webhook_secret